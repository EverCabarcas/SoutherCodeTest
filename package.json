{
  "name": "southerncodetest",
  "version": "1.0.0",
  "description": "The Generic Motors (GM) car company has a terrible API. It returns badly structured JSON which isn't always consistent. Smartcar needs to adapt the API into a cleaner format.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest"
  },
  "keywords": [
    "southern",
    "GM",
    "API",
    "Rest"
  ],
  "author": "Ever Cabarcas <evercabarcasmallarino@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apicache": "1.6.3",
    "axios": "1.7.7",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "helmet": "8.0.0",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "redis": "4.7.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "xss-clean": "0.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@eslint/eslintrc": "3.2.0",
    "babel-jest": "29.7.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "nodemon": "3.1.7",
    "prettier": "3.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
